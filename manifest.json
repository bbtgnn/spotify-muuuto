{
  "name": "Spotify Muuuto",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Chrome extension for silencing tab audio when ads are playing ;)",
  "homepage_url": "https://github.com/bbtgnn/spotify-muuuto",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo"
  },
  "permissions": [
    "tabs",
    "https://*/*"
  ]
}